    "overview": ["./test.new/manual/overview.md"],
    "design": ["./test.new/manual/design.md"],
    "installation": ["./test.new/manual/installation.md"],
    "usage": ["./test.new/manual/usage1.md", "./test.new/manual/usage2.md"],
    "tutorial": ["./test.new/manual/tutorial.md"],
    "configuration": ["./test.new/manual/configuration.md"],
    "example": ["./test.new/manual/example.md"],
    "advanced": ["./test.new/manual/advanced.md"],
    "faq": ["./test.new/manual/faq.md"],
    "changelog": ["./test.new/CHANGELOG.md"]
