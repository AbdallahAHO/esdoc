  "source": "./test/fixture/package/src",
  "destination": "./test/fixture/dest/esdoc",
  "debug": false,
  "index": "./test/fixture/package/README.md",
  "package": "./test/fixture/package/package.json",
  "title": "ESDoc Test Fixture",
