    "tutorial": ["./test/fixture/package/manual/tutorial.md"],
    "configuration": ["./test/fixture/package/manual/configuration.md"],
    "example": ["./test/fixture/package/manual/example.md"],
    "advanced": ["./test/fixture/package/manual/advanced.md"],
    "faq": ["./test/fixture/package/manual/faq.md"],
